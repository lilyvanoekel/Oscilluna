processor add(float32 m = 1.0f)
{
    input event float32 frequencyIn;
    output event float32 frequencyOut;

    event frequencyIn (float32 f)
    {
        frequencyOut <- f + m;
    }
}

processor mul(float32 m = 1.01f)
{
    input event float32 frequencyIn;
    output event float32 frequencyOut;

    event frequencyIn (float32 f)
    {
        frequencyOut <- f * m;
    }
}